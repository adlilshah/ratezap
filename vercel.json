{
  "rewrites": [
    { "source": "/rate/:id", "destination": "/rate/:id.html" },
    { "source": "/badreview/:id", "destination": "/badreview/:id.html" },
    { "source": "/goodreview/:id", "destination": "/goodreview/:id.html" }
  ],
  "headers": [
    {
      "source": "/service-worker.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

